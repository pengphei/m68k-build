<board>
  <properties>
    <description>
      Board:	Freescale MCF51QE64 (50MHz)
CPU:	51qe
    </description>
    <property name="system-clock">
      50227KHz
    </property>
    <property name="core">
      v1
    </property>
  </properties>
  <initialize>
    <!-- disable the watchdog timer in SOPT1 -->
    <write-memory address="0xffff9802" value="0x12" bits="8"/>
    <!-- ICSTRM: TRIM:0x80 -->
    <write-memory address="0xffff803a" value="0x80" bits="8"/>
    <!-- Wait for FLL to lock on frequency -->
    <delay time="200ms"/>
    <!-- ICSSC: DRS:10; DMX32:0; IREFST:1; CLKST:00; OSCINIT:0; FTRIM:0 -->
    <write-memory address="0xffff803b" value="0x90" bits="8"/>
    <!-- Wait for FLL to change frequency scale -->
    <delay time="100ms"/>
  </initialize>
  <memory-map>
    <memory-device address="0x00000000" size="64K" type="flash" device="cfm">
      <description>
	64KBytes Flash
      </description>
    </memory-device>
    <memory-device address="0x00800000" size="8K" type="ram">
      <description>
	8KBytes Internal RAM
      </description>
    </memory-device>
  </memory-map>
</board>
