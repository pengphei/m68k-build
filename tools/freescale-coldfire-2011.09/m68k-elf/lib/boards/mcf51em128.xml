<board>
  <properties>
    <description>
      Board:	Freescale MCF51EM128 (Nucleus)
CPU:	51em
    </description>
    <property name="system-clock">
      16742KHz
    </property>
    <property name="core">
      v1
    </property>
  </properties>
  <initialize>
    <!-- disable the watchdog timer in SOPT1 -->
    <write-memory address="0xffff8281" value="0x10" bits="8"/>
    <!-- ICSTRM: TRIM:0x80 -->
    <write-memory address="0xffff8222" value="0x80" bits="8"/>
    <!-- Wait for FLL to lock on frequency -->
    <delay time="300ms"/>
  </initialize>
  <memory-map>
    <memory-device address="0x00000000" size="64K" type="flash" device="cfm">
      <property name="ipsbar">
	0xffff6f60
      </property>
      <description>
	64KBytes Flash Block 1
      </description>
    </memory-device>
    <memory-device address="0x00010000" size="64K" type="flash" device="cfm">
      <property name="ipsbar">
	0xffff6f80
      </property>
      <description>
	64KBytes Flash Block 2
      </description>
    </memory-device>
    <memory-device address="0x00800000" size="8K" type="ram">
      <description>
	8KBytes Internal RAM
      </description>
    </memory-device>
  </memory-map>
</board>
